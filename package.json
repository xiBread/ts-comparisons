		"compare",
		"comparison",
		"ts",
		"typescript"
	],
	"devDependencies": {
		"@types/node": "^16.11.9",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"prettier": "^2.4.1",
		"tslib": "^2.3.1",
		"typescript": "^4.5.2"
	}
