		"lint:fix": "npm run lint -- --fix",
		"prettier": "prettier -w src"
	},
	"author": "Oscar Lee-Vermeren <oscar.leevermeren@gmail.com>",
	"license": "MIT",
	"keywords": [
		"kotlin",
