{
	"name": "ts-comparisons",
	"version": "1.0.0",
	"description": "A port of Kotlin's Comparisons library.",
	"main": "dist/index.js",
	"typings": "dist/typings/index.d.ts",
	"scripts": {
		"build": "rimraf dist && tsc --build",
		"style": "npm run lint:fix && prettier",
